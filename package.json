{
  "name": "scanner-map",
  "version": "2.0.0",
  "description": "Real-time mapping system for radio calls with transcription and Discord integration",
  "main": "index.js",
  "scripts": {
    "start": "node start-with-deps.js",
    "launch": "node start-with-deps.js",
    "setup": "node index.js",
    "bot": "node bot.js",
    "webserver": "node webserver.js",
    "test": "node tests/run-all-tests.js",
    "test:unit": "node tests/run-all-tests.js --suite=unit",
    "test:integration": "node tests/run-all-tests.js --suite=integration",
    "test:e2e": "node tests/run-all-tests.js --suite=e2e",
    "test:verbose": "node tests/run-all-tests.js --verbose",
    "test:coverage": "node tests/run-all-tests.js --coverage",
    "test:verify": "node tests/verify-setup.js",
    "test:server": "node tests/start-test-server.js",
    "test:regression": "node tests/regression-test.js",
    "test:regression:update": "node tests/regression-test.js --update-baseline"
  },
  "keywords": [
    "scanner",
    "radio",
    "mapping",
    "transcription",
    "discord",
    "sdrtrunk",
    "trunk-recorder"
  ],
  "author": "poisonednumber",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "aws-sdk": "^2.1517.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "busboy": "^1.6.0",
    "csv-parser": "^3.0.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "form-data": "^4.0.0",
    "libsodium-wrappers": "^0.7.13",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "openai": "^6.15.0",
    "prism-media": "^1.3.5",
    "public-ip": "^8.0.0",
    "socket.io": "^4.8.2",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "engines": {
    "node": ">=18.0.0 <21.0.0"
  }
}

